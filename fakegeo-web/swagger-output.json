    "/fakegeo": {
      "get": {
        "summary": "Returns a greeting message",
        "description": "Returns a simple hello message from the FakeGeo API.",
        "responses": {
          "200": {
            "description": "A successful response",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "message": {
                      "type": "string",
                      "example": "Hello, FakeGeo!"
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    "/feature/line": {
      "get": {
        "tags": [
          "Line feature"
        ],
        "summary": "Returns a line geojson Feature",
        "description": "Generates a geographical line.",
        "responses": {
          "200": {
            "description": "A successful response containing the line feature",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
