{
  "name": "fakegeo-api",
  "version": "1.0.0",
  "bin": {
    "fakegeo-api": "bin/fakegeo-api.js"
  },
  "scripts": {
    "clean": "tsc --build --clean",
    "build": "tsc --build",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "serve": "npm run build && cdk deploy --profile shankar-dev-profile",
    "login": "aws sso login --profile shankar-dev-profile",
    "local": "npm run build && cdk synth && sam local invoke FakeGeoFunction -t ./cdk.out/FakegeoApiStack.template.json --event %EVENT_FILE%",
    "api": "cdk synth && sam local start-api -t ./cdk.out/FakegeoApiStack.template.json",
    "generate-docs": "ts-node ./generate-swagger.ts"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.145",
    "@types/jest": "^29.5.12",
    "@types/node": "20.12.7",
    "@types/swagger-jsdoc": "^6.0.4",
    "aws-cdk": "2.143.0",
    "jest": "^29.7.0",
    "swagger-jsdoc": "^6.2.8",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "~5.4.5"
  },
  "dependencies": {
    "@turf/turf": "^7.1.0",
    "aws-cdk-lib": "2.143.0",
    "constructs": "^10.0.0",
    "dotenv": "^16.4.5",
    "source-map-support": "^0.5.21"
  }
}
